%import common.ESCAPED_STRING
%import common.SIGNED_NUMBER    -> NUMBER

%import common.WS
%ignore WS

?value: dict
     | list
     | string
     | NUMBER   -> number
     | "true"   -> true
     | "false"  -> false
     | "null"   -> null

string: ESCAPED_STRING

list: "[" [value ("," value)*] "]"

dict: "{" [pair ("," pair)*] "}"
pair: string ":" value

